aws_region = "us-east-2"
environment = "dev"

# VPC Configuration
vpc_id = "vpc-xxxxxxxxxxxxxxxxx"
database_subnet_ids = ["subnet-xxxxxxxxxxxxxxxxx", "subnet-yyyyyyyyyyyyyyyyy"]
ec2_subnet_id = "subnet-xxxxxxxxxxxxxxxxx"

# Network Access (add your IP)
allowed_cidr_blocks = [""]
allowed_origins = ["http://localhost:3000", "http://localhost:8080"]

# Database Credentials
db_master_username = "postgres"
db_master_password = "CHANGE_THIS_SECURE_PASSWORD"

# Aurora Serverless v2 Capacity (ACU)
aurora_min_capacity = 0.5
aurora_max_capacity = 16

# EC2 Configuration
# Find AMI ID: https://us-east-2.console.aws.amazon.com/ec2/v2/home?region=us-east-2#Images:visibility=public-images
ec2_ami_id = "ami-xxxxxxxxxxxxxxxxx"  # Amazon Linux 2023
ec2_instance_type = "t3.micro"

